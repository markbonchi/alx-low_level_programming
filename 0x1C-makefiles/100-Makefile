CC= gcc
SRC= main.c school.c
OBJ= main.o school.o
NAME= school
RM= rm -f
CFLAGS= -Wall -Werror -Wextra -pedantic

.PHONY: all clean oclean fcleaan re

all: $(SRC) m.h
	$(CC) -c $(SRC)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: fclean all
